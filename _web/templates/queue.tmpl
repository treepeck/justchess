{{ define "content" }}
<div id="mainContainer" data-page="queue">
  <div class="queue-info-header">Finding an opponent...</div>

  <div class="queue-info-container">
    <div id="clientsCounter">Players in queue: ?</div>
    <div class="queue-info-control">
      Control: {{ .Data.Control }}m + {{ .Data.Bonus }}s
    </div>
    <div>Rating: {{ .Base.Player.Rating }}</div>
    <div id="countUpTimer">00:00</div>
  </div>

  <div id="ping">Ping: ? ms</div>
</div>
{{ end }}